	public static void main(String[] args) {
		// TODO Auto-generated method stub
	    Scanner scan = new Scanner(System.in);
	    int level=scan.nextInt();
		star(level);
		
	 }
   public static void star(int level) {
	    
	    for(int i=1 ; i<=level ; i++) 
	    {
		
		   for(int j=1 ; j<=2*i-1;j++)
		   {
		       System.out.print("*");	  

		   } 
	       System.out.println(" ");	  

	    }
   }