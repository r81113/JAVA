  Scanner input = new Scanner(System.in);

      while(true){
    	
    	int day = input.nextInt();
    	
    	 switch(day){
    	 
    
            case 1:
    	        System.out.println("打人");
    	        break;
                
            case 2:
    	        System.out.println("運動");
                 break;
            case 3:
    	        System.out.println("吃飯");
                 break;
      
      
          }
    	
		
      }